<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

	<class name="org.openbp.server.test.PojoPersistedComplexParam"
		table="PersistedComplexParam"
		optimistic-lock="none"
		lazy="false">

		<id name="Id" type="integer">
			<column name="PCP_ID" not-null="true"/>
			<generator class="hilo">
				<param name="table">OpenBPHILO</param>
				<param name="column">NEXT_VALUE</param>
			</generator>
		</id>

		<property name="Title" type="string">
			<column name="PCP_TITLE" length="40"/>
		</property>

		<property name="Result" type="integer">
			<column name="PCP_RESULT"/>
		</property>

	</class>

</hibernate-mapping>
